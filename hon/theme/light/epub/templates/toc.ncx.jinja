<?xml version='1.0' encoding='utf-8'?>
<ncx xmlns="http://www.daisy.org/z3986/2005/ncx/" version="2005-1" xml:lang="UND">
  <head>
    <meta name="dtb:uid" content="{{ isbn }}" />
    <meta name="dtb:depth" content="1" />
    <meta name="dtb:totalPageCount" content="0" />
    <meta name="dtb:maxPageNumber" content="0" />
  </head>
  <docTitle>
      <text>{{ title }}</text>
  </docTitle>
  <docAuthor>
      <text>{{ author }}</text>
  </docAuthor>
  <navMap>
    <navPoint id="titlepage" playOrder="1">
      <navLabel>
        <text>Cover</text>
      </navLabel>
      <content src="titlepage.xhtml"/>
    </navPoint>
    {% for part in summary.all_parts %}
    <navPoint id="id{{ loop.index }}" playOrder="{{ loop.index + 1 }}">
      <navLabel>
          <text>{{ part.name }}</text>
      </navLabel>
      <content src="{{ part.link_name }}.xhtml"/>
   </navPoint>
   {% endfor %}

  </navMap>
</ncx>
